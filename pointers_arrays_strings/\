#include <stdio.h>
#include <unistd.h>

void rev_string(char *s)
{
	int i = 0;
	int j = i;

	while (s[i] != '\0')
	{
		write(1, &s[i], 1);
		i++;
	}

	while (j > 0)
	{
		i--;
		write(1, &s[j], 1);
	}
}

int main(void)
{
    char s[10] = "My School";

    printf("%s\n", s);
    rev_string(s);
    printf("%s\n", s);
    return (0);
}
